<div class="batch-edit-container">
  <div class="row border-btm">
    <div class="col col-sm-3 col-md-3 col-lg-3">
      <%= react_component("Dropdown",
        {
          title: 'Appendix',
          placeholder: "Please select appendix value",
          data: ['a', 'b', 'c'],
          enabled: true
        })
      %>
    </div>
    <div class="col col-sm-3 col-md-3 col-lg-3">
      <%= react_component("Dropdown",
        {
          title: 'Taxon name',
          placeholder: "Please select appendix value",
          data: ['a', 'b', 'c'],
          enabled: true
        })
      %>
    </div>
  </div>
  <div class="row border-btm">
    <div class="col col-sm-3 col-md-3 col-lg-3">
      <%= react_component("Dropdown",
        {
          title: 'Term',
          placeholder: "Please select appendix value",
          data: ['a', 'b', 'c'],
          enabled: true
        })
      %>
    </div>
    <div class="col col-sm-3 col-md-3 col-lg-3">
      <%= react_component("Dropdown",
        {
          title: 'Quantity <span>(Qty)</span>',
          placeholder: "Please select appendix value",
          data: ['a', 'b', 'c'],
          enabled: true
        })
      %>
    </div>
    <div class="col col-sm-3 col-md-3 col-lg-3">
      <%= react_component("Dropdown",
        {
          title: 'Unit',
          placeholder: "Please select appendix value",
          data: ['a', 'b', 'c'],
          enabled: true
        })
      %>
    </div>
    <div class="col col-sm-3 col-md-3 col-lg-3">
      <%= react_component("Dropdown",
        {
          title: 'Trading partner',
          placeholder: "Please select appendix value",
          data: ['a', 'b', 'c'],
          enabled: true
        })
      %>
    </div>
  </div>
  <div class="row border-btm">
    <div class="col col-sm-3 col-md-3 col-lg-3">
      <%= react_component("Dropdown",
        {
          title: 'Origin',
          placeholder: "Please select appendix value",
          data: ['a', 'b', 'c'],
          enabled: true
        })
      %>
    </div>
    <div class="col col-sm-3 col-md-3 col-lg-3">
      <%= react_component("InputBox",
        {
          title: 'Import Permit',
          enabled: true,
          blankCheckbox: true
        })
      %>
    </div>
    <div class="col col-sm-3 col-md-3 col-lg-3">
      <%= react_component("InputBox",
        {
          title: 'Export Permit',
          enabled: false,
          blankCheckbox: true
        })
      %>
    </div>
    <div class="col col-sm-3 col-md-3 col-lg-3">
      <%= react_component("InputBox",
        {
          title: 'Origin Permit',
          enabled: true,
          blankCheckbox: true
        })
      %>
    </div>
  </div>
  <div class="row border-btm">
    <div class="col col-sm-3 col-md-3 col-lg-3">
      <%= react_component("Dropdown",
        {
          title: 'Appendix',
          placeholder: "Please select appendix value",
          data: ['a', 'b', 'c'],
          enabled: false
        })
      %>
    </div>
    <div class="col col-sm-3 col-md-3 col-lg-3">
      <%= react_component("Dropdown",
        {
          title: 'Appendix',
          placeholder: "Please select appendix value",
          data: ['a', 'b', 'c'],
          enabled: true,
          blankCheckbox: true
        })
      %>
    </div>
    <div class="col col-sm-3 col-md-3 col-lg-3">
      <%= react_component("InputBox",
        {
          title: 'Year',
          enabled: true,
        })
      %>
    </div>
  </div>

  <div class="buttons right">
  </div>
</div>
